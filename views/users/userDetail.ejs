<%- include('../partials/head') %>
<body>
    <%- include('../partials/header') %>
    <!-- DATOS DEL USUARIO-->
    <main>
        <h1><%= usuarioBuscado.id  %> <%= usuarioBuscado.nombre  %> <%= usuarioBuscado.apellido  %></h1>
        <div>Avatar - <img src="../../img/userImages/<%= usuarioBuscado.avatar ? usuarioBuscado.avatar : 'default-image.png' %>" alt=" <%= usuarioBuscado.nombre  %>"></div>
        <!-- <div>Avatar - <img src="<%= usuarioBuscado.avatar %>" alt=" <%= usuarioBuscado.nombre  %>"></div> -->
